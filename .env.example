# SHEPARD Protocol Environment Configuration
# Copy this file to .env and fill in your values

# Private key for deployment (DO NOT COMMIT YOUR ACTUAL KEY)
PRIVATE_KEY=your_private_key_here

# Metal L2 RPC URLs
METAL_RPC_URL=https://rpc.metall2.com
METAL_TESTNET_RPC=https://tahoe.metalblockchain.org/ext/bc/C/rpc

# Block Explorer API (optional, for contract verification)
METAL_EXPLORER_API=your_api_key_here
